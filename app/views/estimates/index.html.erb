<p id="notice"><%= notice %></p>

<h1>Estimates</h1>

<table>
  <thead>
    <tr>
      <th>Currently listed</th>
      <th>Prop owner name</th>
      <th>Contact name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Street</th>
      <th>Street2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Beds</th>
      <th>Baths</th>
      <th>Home sf</th>
      <th>Prop sf</th>
      <th>Num garages</th>
      <th>Pool</th>
      <th>Spa</th>
      <th>Upgrades</th>
      <th>Damage</th>
      <th>Photo</th>
      <th>Tod call</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estimates.each do |estimate| %>
      <tr>
        <td><%= estimate.currently_listed %></td>
        <td><%= estimate.prop_owner_name %></td>
        <td><%= estimate.contact_name %></td>
        <td><%= estimate.email %></td>
        <td><%= estimate.phone %></td>
        <td><%= estimate.street %></td>
        <td><%= estimate.street2 %></td>
        <td><%= estimate.city %></td>
        <td><%= estimate.state %></td>
        <td><%= estimate.zip %></td>
        <td><%= estimate.beds %></td>
        <td><%= estimate.baths %></td>
        <td><%= estimate.home_sf %></td>
        <td><%= estimate.prop_sf %></td>
        <td><%= estimate.num_garages %></td>
        <td><%= estimate.pool %></td>
        <td><%= estimate.spa %></td>
        <td><%= estimate.upgrades %></td>
        <td><%= estimate.damage %></td>
        <td><%= estimate.photo %></td>
        <td><%= estimate.tod_call %></td>
        <td><%= link_to 'Show', estimate %></td>
        <td><%= link_to 'Edit', edit_estimate_path(estimate) %></td>
        <td><%= link_to 'Destroy', estimate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Estimate', new_estimate_path %>
